{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "excludePattern": "",
        "includePattern": ".*\\.(js|html)$",
        "include": [
            "./build"
        ]
    },
    "plugins": [],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "systemName": "Discord.ts",
        "copyright": "Nimplex & Mateuszkdev",
        "theme": "Simplex",
        "includeDate": false
    },
    "opts": {
        "recurse": true,
        "template": "./node_modules/better-docs",
        "readme": "readme.md",
        "destination": "./docs/",
        "encoding": "utf8"
    }
}